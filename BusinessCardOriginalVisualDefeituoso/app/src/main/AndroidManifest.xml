<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gusoliveira21.businesscard">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BusinessCard">
        <activity
            android:name=".acesso.MeiosDeAcessoActivity"
            android:label="@string/title_activity_meios_de_acesso"
            android:theme="@style/Theme.BusinessCard.NoActionBar"></activity>
        <activity android:name=".TesteMainActivity" />
        <activity android:name=".ui.AddBusinessCardActivity" />
        <activity
            android:name=".ui.MainActivity"
            android:label="My Custom AppIntro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>